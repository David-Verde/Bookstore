{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "import/extensions": [
         "error",
         "ignorePackages",
         {
           "js": "never",
           "jsx": "never",
           "ts": "never",
           "tsx": "never"
         }
      ]
   },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }